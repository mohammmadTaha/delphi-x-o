{$R *.dfm}

{ TForm1 }

{ TForm1 }                                

procedure TForm1.enable_false;
begin
btn1.Enabled :=false;
btn2.Enabled :=false;
btn3.Enabled :=false;
btn4.Enabled :=false;
btn5.Enabled :=false;
btn6.Enabled :=false;
btn7.Enabled :=false;
btn8.Enabled :=false;
btn9.Enabled :=false;
end;

procedure TForm1.scorekepeer;
var x,o:Integer;
begin
  x:=StrToInt(lblx.Caption);
  o:=StrToInt(lblo.Caption);
end;

end.
